local model = script.Parent
local baseplate = model:FindFirstChild("Baseplate")
if not baseplate or not baseplate:IsA("BasePart") then
	error("script isnt working for u")
end

local blocksFolder = model:FindFirstChild("Blocks") or Instance.new("Folder", model)
blocksFolder.Name = "Blocks"

local eggHeight = 50
local eggBaseOffset = 24
local partSize = Vector3.new(0.5, 0.5, 0.5)
local dx, dz, dy = partSize.X, partSize.Z, partSize.Y

local baseplateTopY = baseplate.Position.Y + (baseplate.Size.Y / 2)
local cx, cz = baseplate.Position.X, baseplate.Position.Z

local p0 = 5
local p1 = 28
local p2 = 28
local p3 = 10

local function R(t)
	local mt = 1 - t
	return (mt^3)*p0 + 3*(mt^2)*t*p1 + 3*mt*(t^2)*p2 + (t^3)*p3
end

local count = 0
for y = 0, eggHeight, dy do
	local t = y / eggHeight
	local radius = R(t)
	for x = -radius, radius, dx do
		for z = -radius, radius, dz do
			if (x*x + z*z) <= (radius * radius) then
				local part = Instance.new("Part")
				part.Size = partSize
				part.Anchored = false
				part.Color = Color3.new(1, 1, 1)
				part.CFrame = CFrame.new(cx + x, baseplateTopY + eggBaseOffset + y, cz + z)
				part.Parent = blocksFolder
				
				local weld = Instance.new("WeldConstraint")
				weld.Part0 = baseplate
				weld.Part1 = part
				weld.Parent = part
				
				count = count + 1
				if count % 1000 == 0 then
					task.wait()
				end
			end
		end
	end
end

print("THE EGG was generated with", count, "parts.. ur pc is gud :>")
